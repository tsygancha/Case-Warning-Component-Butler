    <decisions>
        <name>Determine_Color_Scheme</name>
        <label>Determine Color Scheme</label>
        <locationX>446</locationX>
        <locationY>650</locationY>
        <defaultConnector>
            <targetReference>Assign_Error</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Assign Error Status</defaultConnectorLabel>
        <rules>
            <name>Assign_Successful_Status</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Is_Cases_Current_Month_Less_Than_Allowed</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Is_Cases_Previous_Month_Less_Than_Half_Allowed</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Success</targetReference>
            </connector>
            <label>Assign Successful Status</label>
        </rules>
        <rules>
            <name>Assign_Warning_Status</name>
            <conditionLogic>(1 AND 2) OR (3 AND 4)</conditionLogic>
            <conditions>
                <leftValueReference>Is_Cases_Current_Month_Less_Than_Allowed</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Is_Cases_Previous_Month_Greater_Than_Half_Allowed</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Is_Cases_Current_Month_25Percent_Greater_Than_Allowed</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Is_Cases_Previous_Month_Less_Than_75Percent_Allowed</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Warning</targetReference>
            </connector>
            <label>Assign Warning Status</label>
        </rules>
        <rules>
            <name>Assign_Problem_Status</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Is_Cases_Current_Month_Greater_Than_Allowed</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Problem</targetReference>
            </connector>
            <label>Assign Problem Status</label>
        </rules>
    </decisions>
