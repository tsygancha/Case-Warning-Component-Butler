    <formulas>
        <name>Is_Cases_Current_Month_25Percent_Greater_Than_Allowed</name>
        <dataType>Boolean</dataType>
        <expression>{!Get_Account_Records.Cases_for_Current_Month__c} &gt; 1.25 * {!Get_Account_Records.Count_of_Cases_Monthly_Allowed__c}</expression>
    </formulas>
    <formulas>
        <name>Is_Cases_Current_Month_Greater_Than_Allowed</name>
        <dataType>Boolean</dataType>
        <expression>{!Get_Account_Records.Cases_for_Current_Month__c} &gt;={!Get_Account_Records.Count_of_Cases_Monthly_Allowed__c}</expression>
    </formulas>
    <formulas>
        <name>Is_Cases_Current_Month_Less_Than_Allowed</name>
        <dataType>Boolean</dataType>
        <expression>{!Get_Account_Records.Cases_for_Current_Month__c} &lt; {!Get_Account_Records.Count_of_Cases_Monthly_Allowed__c}</expression>
    </formulas>
    <formulas>
        <name>Is_Cases_Previous_Month_Greater_Than_Half_Allowed</name>
        <dataType>Boolean</dataType>
        <expression>{!Get_Account_Records.Cases_for_Previous_Month__c} &gt; 0.5 * {!Get_Account_Records.Count_of_Cases_Monthly_Allowed__c}</expression>
    </formulas>
    <formulas>
        <name>Is_Cases_Previous_Month_Less_Than_75Percent_Allowed</name>
        <dataType>Boolean</dataType>
        <expression>{!Get_Account_Records.Cases_for_Previous_Month__c} &lt; 0.75 * {!Get_Account_Records.Count_of_Cases_Monthly_Allowed__c}</expression>
    </formulas>
    <formulas>
        <name>Is_Cases_Previous_Month_Less_Than_Half_Allowed</name>
        <dataType>Boolean</dataType>
        <expression>{!Get_Account_Records.Cases_for_Previous_Month__c} &lt;= 0.5 * {!Get_Account_Records.Count_of_Cases_Monthly_Allowed__c}</expression>
    </formulas>
